{
    "name": "quanta/container-compilation",
    "type": "library",
    "description": "Library for compiling quanta Psr-11 container factory maps",
    "keywords": [
        "psr-11",
        "container",
        "dependency-injection",
        "compilation"
    ],
    "license": "MIT",
    "homepage": "https://github.com/quanta/container-compilation",
    "support": {
        "issues": "https://github.com/quanta/container-compilation/issues",
        "source": "https://github.com/quanta/container-compilation"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0",
        "jeremeamia/SuperClosure": "^2.4",
        "quanta/container-factories": "^0.5.0"
    },
    "require-dev": {
        "eloquent/phony-kahlan": "^2.0",
        "kahlan/kahlan": "^4.0",
        "phpstan/phpstan": "^0.10.8"
    },
    "autoload": {
        "psr-4": {
            "Quanta\\Container\\": "src"
        }
    }
}
